<%- include('../partials/header') %>
<table id="list">
  <thead>
    <tr>
      <th>Recipe Name</th>
      <th>Sub<br>Category</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% recipes.forEach(function(m) { %>
      <tr>
        <td><%= m.recipeName %></td>
        <td><%= m.subCategory %></td>
        <td><a href="/recipes/<%= m._id %>">Recipe</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>
